{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Stock</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ entity.quantity }}</td>
            </tr>
            <tr>
                <th>Basequantity</th>
                <td>{{ entity.baseQuantity }}</td>
            </tr>
            <tr>
                <th>Lot</th>
                <td>{{ entity.lot }}</td>
            </tr>
            <tr>
                <th>Sn</th>
                <td>{{ entity.sn }}</td>
            </tr>
            <tr>
                <th>Expirydate</th>
                <td>{{ entity.expiryDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Bestbeforedate</th>
                <td>{{ entity.bestBeforeDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Reciveddate</th>
                <td>{{ entity.recivedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Productiondate</th>
                <td>{{ entity.productionDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updateddate</th>
                <td>{{ entity.updatedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Objectid</th>
                <td>{{ entity.objectId }}</td>
            </tr>
            <tr>
                <th>Objecttype</th>
                <td>{{ entity.objectType }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('stock') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('stock_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
