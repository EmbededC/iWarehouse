{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ProductAlias</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updateddate</th>
                <td>{{ entity.updatedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Objectid</th>
                <td>{{ entity.objectId }}</td>
            </tr>
            <tr>
                <th>Objecttype</th>
                <td>{{ entity.objectType }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('productalias') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('productalias_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
