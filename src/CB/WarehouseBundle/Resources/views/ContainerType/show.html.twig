{% extends '::base.html.twig' %}

{% block body -%}
    <h1>ContainerType</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Maxweight</th>
                <td>{{ entity.maxWeight }}</td>
            </tr>
            <tr>
                <th>Sizex</th>
                <td>{{ entity.sizeX }}</td>
            </tr>
            <tr>
                <th>Sizey</th>
                <td>{{ entity.sizeY }}</td>
            </tr>
            <tr>
                <th>Sizez</th>
                <td>{{ entity.sizeZ }}</td>
            </tr>
            <tr>
                <th>Createddate</th>
                <td>{{ entity.createdDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updateddate</th>
                <td>{{ entity.updatedDate|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('containertype') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('containertype_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
